    <owl:Class rdf:about="http://purl.obolibrary.org/obo/BFO_0000001">
        <obo:BFO_0000179>entity</obo:BFO_0000179>
        <obo:BFO_0000180>Entity</obo:BFO_0000180>
        <obo:IAO_0000112 xml:lang="en">Julius Caesar</obo:IAO_0000112>
